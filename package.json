{
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "start": "pnpm -C example dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react"
      ]
    }
  },
  "dependencies": {
    "@welkin-ui/components": "workspace:*",
    "@welkin-ui/hooks": "workspace:*",
    "@welkin-ui/utils": "workspace:*",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/test-utils": "^2.2.3",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.0",
    "sass": "^1.55.0",
    "vite": "^3.2.2",
    "vitest": "^0.25.2"
  }
}
